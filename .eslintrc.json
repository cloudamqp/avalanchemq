{
    "env": {
      "es2020": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "plugins": [
        "html"
    ],
    "settings": {
        "html/indent": "+2"
    },
    "rules": {
        "no-console": "off"
    }
}
